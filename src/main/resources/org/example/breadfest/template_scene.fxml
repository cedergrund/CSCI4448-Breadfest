<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<?import java.lang.Double?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"

            prefWidth="600" prefHeight="400">

   <!-- Label at the center -->
   <Label text="Welcome to the Room Test!" layoutX="-1.0" layoutY="-1.0" AnchorPane.leftAnchor="270.0" AnchorPane.rightAnchor="270.0" AnchorPane.topAnchor="90.0" AnchorPane.bottomAnchor="90.0" >
      <layoutX>
         <Double fx:constant="MAX_VALUE"/>
      </layoutX>
      <layoutY>
         <Double fx:constant="MAX_VALUE"/>
      </layoutY>
   </Label>

   <!-- Button at the top center -->
   <Button text="North" AnchorPane.topAnchor="20" AnchorPane.leftAnchor="700" AnchorPane.rightAnchor="700"/>

   <!-- Button at the bottom center -->
   <Button text="South" AnchorPane.bottomAnchor="20" AnchorPane.leftAnchor="700" AnchorPane.rightAnchor="700"/>

   <!-- Button at the left center -->
   <Button text="West" AnchorPane.topAnchor="300" AnchorPane.leftAnchor="20" AnchorPane.bottomAnchor="300"/>

   <!-- Button at the right center -->
   <Button text="East" AnchorPane.topAnchor="300" AnchorPane.rightAnchor="20" AnchorPane.bottomAnchor="300"/>
</AnchorPane>